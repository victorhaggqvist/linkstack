{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "quotes": [1, "single", "avoid-escape"],
        "curly": [1, "multi-line"],
        "no-extra-strict": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": [2, {"vars": "local", "args": "after-used"}],
        "no-console": [1]
    },
    "extends": "eslint:recommended"
}
